{
    "name": "miladev/lara-swagger",
    "description": "Swagger model generator based on Laravel migrations",
    "keywords": [ "swagger", "swagger model", "laravel swagger" ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Miladev\\LaravelSwagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miladev\\LaravelSwagger\\LaravelSwaggerServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.x-dev",
            "dev-master": "1.x-dev"
        }
    },
    "authors": [
        {
            "name": "Milad",
            "email": "miladev95@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0"
    }
}
